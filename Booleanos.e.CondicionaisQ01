//Faça um Programa que peça dois números e imprima o maior deles, e informe caso eles sejam iguais.

  console.clear();
const prompt = require('prompt-sync')();

let num1 = +prompt('Digite um número ');
let num2 = +prompt('Digite outro número ');

    if (num1 > num2) {
      console.log(`O maior número é o ${num1}.`);
}
    else if (num2 > num1) {
      console.log(`O maior número é o ${num2}.`);
} 
    else  {(num1 == num2) 
      console.log(`Os números ${num1} e ${num2} são iguais.`)
}